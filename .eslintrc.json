{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "lines-between-class-members": "off",
        "class-methods-use-this": "off",
        "import/prefer-default-export": "off",
        "import/extensions": ["error", "never", {"ignorePackages": true } ]
    }
}
